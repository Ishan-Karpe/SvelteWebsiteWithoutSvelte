{
  "version": 2,
  "functions": {
    "api/generate.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/",
      "destination": "/src/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/src/$1"
    }
  ]
}